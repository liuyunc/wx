<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wx.android">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.CAMERA"/>

    <uses-feature android:name="android.hardware.camera"/>
    <uses-feature android:name="android.hardware.camera.autofocus"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/timelogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.wx.android.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.wx.android.MainActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            ></activity>

        <activity android:name="com.wx.android.fx.DecoderActivity"

            ></activity>

        <activity android:name=".testActivity">


        </activity>
        <activity android:name=".EcgActivity"></activity>
        <activity android:name=".HeartActivity"></activity>
        <activity android:name=".HistoryActivity"></activity>
    </application>

</manifest>
